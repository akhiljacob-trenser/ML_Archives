   "source": [
    "import torch\n",
    "from torch.autograd import Variable\n",
    "\n",
    "x=Variable(torch.ones(2,2), requires_grad=True)\n",
    "y=x+1\n",
    "z=2*y*y+3\n",
    "mean_val=z.mean()\n",
    "mean_val.backward() #back prop\n",
    "print(mean_val)\n",
    "print(x.grad)"
   ]
  }
